@model IEnumerable<ProyectoASE.Models.PacientesModel>

@{
    ViewData["Title"] = "Caries";
}

<h1>Caries</h1>

<p>
    @Html.ActionLink("Regresar a listado", "Index", new { })
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DPI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Age)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneN)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastAppoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NextAppoint)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DPI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Age)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastAppoint)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NextAppoint)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>          
        </tr>
}
    </tbody>
</table>
